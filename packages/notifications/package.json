{
	"name": "@aws-amplify/notifications",
	"version": "0.1.0",
	"description": "Notifications category of aws-amplify",
	"main": "./lib/index.js",
	"module": "./lib-esm/index.js",
	"typings": "./lib-esm/index.d.ts",
	"sideEffects": [
		"./src/Notifications.ts",
		"./lib/Notifications.js",
		"./lib-esm/Notifications.js",
		"./dist/aws-amplify-notifications.js",
		"./dist/aws-amplify-notifications.min.js"
	],
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"test": "tslint 'src/**/*.ts' && jest -w 1 --coverage --passWithNoTests",
		"test:watch": "tslint 'src/**/*.ts' && jest -w 1 --watch",
		"build-with-test": "npm run clean && npm test && tsc && webpack",
		"build:cjs": "node ./build es5 && webpack && webpack --config ./webpack.config.dev.js",
		"build:esm": "node ./build es6",
		"build:cjs:watch": "node ./build es5 --watch",
		"build:esm:watch": "node ./build es6 --watch",
		"build": "npm run clean && npm run build:esm && npm run build:cjs",
		"clean": "rimraf lib-esm lib dist",
		"format": "echo \"Not implemented\"",
		"lint": "tslint 'src/**/*.ts'"
	},
	"react-native": {
		"./lib/index": "./lib-esm/index.js"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/aws-amplify/amplify-js.git"
	},
	"author": "Amazon Web Services",
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/aws/aws-amplify/issues"
	},
	"homepage": "https://docs.amplify.aws/",
	"dependencies": {
		"@aws-amplify/cache": "4.0.22",
		"@aws-amplify/core": "4.3.2",
		"@aws-sdk/client-pinpoint": "^3.33.0",
		"lodash": "^4.17.21"
	},
	"devDependencies": {
		"@babel/core": "^7.12.9",
		"@babel/runtime": "^7.12.5",
		"@types/jest": "^26.0.23",
		"@types/react": "^17.0.11",
		"@types/react-test-renderer": "^17.0.1",
		"babel-jest": "^26.6.3",
		"eslint": "7.14.0",
		"jest": "^26.6.3",
		"rimraf": "^3.0.2",
		"typescript": "^4.3.2"
	},
	"jest": {
		"globals": {
			"ts-jest": {
				"diagnostics": false,
				"tsConfig": {
					"lib": [
						"es5",
						"es2015",
						"dom",
						"esnext.asynciterable",
						"es2017.object"
					],
					"allowJs": true
				}
			}
		},
		"transform": {
			"^.+\\.(js|jsx|ts|tsx)$": "ts-jest"
		},
		"testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"json",
			"jsx"
		],
		"testEnvironment": "jsdom",
		"testURL": "http://localhost/",
		"coverageThreshold": {
			"global": {
				"branches": 0,
				"functions": 0,
				"lines": 0,
				"statements": 0
			}
		},
		"coveragePathIgnorePatterns": [
			"/node_modules/",
			"dist",
			"lib",
			"lib-esm"
		]
	}
}
