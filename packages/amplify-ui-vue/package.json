{
	"name": "@aws-amplify/ui-vue",
	"sideEffects": true,
	"version": "0.2.25",
	"description": "Vue specific wrapper for @aws-amplify/ui-components",
	"publishConfig": {
		"access": "public"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
    "require": "./dist/index.js"
	},
	"keywords": ["aws", "amplify", "library", "ui", "user", "interface", "vue"],
	"files": [
		"dist/"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/aws-amplify/amplify-js.git"
	},
	"dependencies": {
		"@aws-amplify/ui-components": "^0.9.1"
	},
	"scripts": {
		"build": "npm run clean && npm run compile",
		"clean": "rm -rf dist",
		"compile": "npm run tsc",
		"tsc": "tsc -p ."
	}
}
