{
    "name": "aws-amplify-vscode",
    "displayName": "AWS Amplify API",
    "description": "AWS Amplify API code snippets",
    "version": "0.0.1",
    "publisher": "AWS",
    "private": true,
    "engines": {
        "vscode": "^1.22.0"
    },
    "categories": [
        "Snippets"
    ],
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "contributes": {
        "snippets": [
            {
                "language": "javascript",
                "path": "./snippets/api.json"
            },
            {
                "language": "javascript",
                "path": "./snippets/auth.json"
            },
            {
                "language": "javascript",
                "path": "./snippets/configure.json"
            },
            {
                "language": "javascript",
                "path": "./snippets/storage.json"
            },
            {
                "language": "javascript",
                "path": "./snippets/analytics.json"
            },
            {
                "language": "javascript",
                "path": "./snippets/pubsub.json"
            },
            {
