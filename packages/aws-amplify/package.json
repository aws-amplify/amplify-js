{
  "name": "aws-amplify",
  "version": "1.1.28",
  "description": "AWS Amplify is a JavaScript library for Frontend and mobile developers building cloud-enabled applications.",
  "main": "./dist/index.js",
  "module": "./esm/index.js",
  "typings": "./esm/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "test": "tslint 'src/**/*.ts' && jest --coverage --maxWorkers 2",
    "build-with-test": "npm run clean && npm test && tsc && webpack -p",
    "build:cjs": "node ./build es5 && node ./build rollup",
    "build:esm": "node ./build es6",
    "build": "npm run clean && npm run build:cjs && npm run build:esm",
    "clean": "rimraf lib lib-esm dist",
    "format": "tsfmt --useTsfmt tsfmt.json -r src/**/*.ts",
    "lint": "tslint 'src/**/*.ts'",
    "generate-docs-local": "typedoc --out docs src",
    "generate-docs-root": "typedoc --out ../../docs src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aws/aws-amplify.git"
  },
  "author": "Amazon Web Services",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/aws/aws-amplify/issues"
  },
  "homepage": "https://github.com/aws/aws-amplify#readme",
  "devDependencies": {
  },
  "dependencies": {
    "@aws-amplify/analytics": "1.2.16",
    "@aws-amplify/api": "1.0.35",
    "@aws-amplify/auth": "1.2.24",
    "@aws-amplify/cache": "1.0.27",
    "@aws-amplify/core": "1.0.27",
    "@aws-amplify/interactions": "1.0.30",
    "@aws-amplify/pubsub": "1.0.28",
    "@aws-amplify/storage": "1.0.30",
    "@aws-amplify/ui": "1.0.19",
    "@aws-amplify/xr": "0.1.17"
  }
}
