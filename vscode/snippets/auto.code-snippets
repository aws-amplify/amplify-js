        "body": [
            "export const handler = async (event, context) => {",
            "\tif (event.request.privateChallengeParameters.answer === event.request.challengeAnswer) {",
            "\t\tevent.response.answerCorrect = true;",
            "\t} else {",
            "\t\tevent.response.answerCorrect = false;",
            "\t}",
            "",
            "\treturn event;",
            "};",
        ]
    },
